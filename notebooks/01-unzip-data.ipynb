   "source": [
    "# Unzip files stored in data/\n",
    "for fname in glob.glob(\"../data/*.zip\"):\n",
    "    dname = os.path.basename(fname).split(\".\")[0]\n",
    "    cmd = \"unzip %s -d ../data/%s\" % (fname, dname)\n",
    "    !$cmd"
   ]
  }
